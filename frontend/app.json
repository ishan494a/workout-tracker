{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/AppIcon~ios-marketing.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "BACKEND_URL": "http://192.168.2.19:8080",
      "APP_ENV": "development",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c3d58b31-7fb4-4a4a-b53b-87dae25f96d3"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ishan494a.flare"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/AppIcon~ios-marketing.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/AppIcon~ios-marketing.png",
      "http": {
        "protocol": "http"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/AppIcon~ios-marketing.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c3d58b31-7fb4-4a4a-b53b-87dae25f96d3"
    },
    "owner": "ishan494a"
  }
}
